{
	"name": "systemtap-syntax",
	"displayName": "systemtap-syntax",
	"license": "MIT",
	"description": "SystemTap syntax highlighting support and auto completion ",
	"version": "0.3.6",
	"publisher": "nzh21",
	"repository": {
		"type": "git",
		"url": "git@github.com:nzh63/systemtap-syntax.git"
	},
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:systemtap"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "systemtap",
				"aliases": [
					"SystemTap",
					"systemtap"
				],
				"extensions": [
					".stp"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "systemtap",
				"scopeName": "source.systemtap",
				"path": "./syntaxes/systemtap.tmLanguage.json",
				"embeddedLanguages": {
					"meta.embedded.block.c": "c"
				},
				"tokenTypes": {
					"meta.embedded.block.c": "other"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint ./src --ext .js,.ts && tsc --noEmit",
		"watch": "tsc -watch -p ./",
		"package": "yarn run compile && vsce package --yarn",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.4.0",
		"@babel/plugin-proposal-class-properties": "^7.4.0",
		"@babel/plugin-proposal-numeric-separator": "^7.2.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.4.0",
		"@babel/plugin-transform-runtime": "^7.6.2",
		"@babel/preset-env": "^7.4.1",
		"@babel/preset-typescript": "^7.3.3",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"@typescript-eslint/eslint-plugin": "^2.4.0",
		"@typescript-eslint/parser": "^2.4.0",
		"eslint": "^6.5.1",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vsce": "^1.68.0",
		"vscode-test": "^1.2.0"
	}
}
